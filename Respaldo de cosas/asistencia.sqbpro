    CASE 
        WHEN sueldo_base &gt;= 2500000 THEN 'Alto (2.5M+)'
        WHEN sueldo_base &gt;= 1500000 THEN 'Medio-Alto (1.5M-2.5M)'
        WHEN sueldo_base &gt;= 1000000 THEN 'Medio (1M-1.5M)'
        WHEN sueldo_base &gt;= 600000 THEN 'Medio-Bajo (600K-1M)'
        ELSE 'Básico (&lt;600K)'
    END as rango_salarial,
    COUNT(*) as cantidad_cargos,
    MIN(sueldo_base) as sueldo_minimo,
    MAX(sueldo_base) as sueldo_maximo,
    AVG(sueldo_base) as sueldo_promedio
FROM cargos 
GROUP BY 
    CASE 
        WHEN sueldo_base &gt;= 2500000 THEN 'Alto (2.5M+)'
        WHEN sueldo_base &gt;= 1500000 THEN 'Medio-Alto (1.5M-2.5M)'
        WHEN sueldo_base &gt;= 1000000 THEN 'Medio (1M-1.5M)'
        WHEN sueldo_base &gt;= 600000 THEN 'Medio-Bajo (600K-1M)'
        ELSE 'Básico (&lt;600K)'
    END
ORDER BY MIN(sueldo_base) DESC;

-- Ver ejemplos de asignaciones por tipo de cargo
SELECT nombre, sueldo_base 
FROM cargos 
WHERE sueldo_base &gt; 0 
ORDER BY sueldo_base DESC 
LIMIT 20;</sql><current_tab id="0"/></tab_sql></sqlb_project>
